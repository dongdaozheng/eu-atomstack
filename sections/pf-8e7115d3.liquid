                  <script>
                    // 播放帧动画
                    frameAnimate();

                    function debounce(fn, delay = 1000) {
                      let timer;
                      return function () {
                        window.clearTimeout(timer);
                        timer = setTimeout(() => {
                          fn.apply(this, arguments);
                          window.clearTimeout(timer);
                        }, delay);
                      };
                    }

                    // 进行函数防抖
                    let debounced = debounce(function () {
                      frameAnimate();
                    });

                    // 监听resize事件
                    window.addEventListener('resize', debounced);

                    function frameAnimate() {
                      var imgPath = 'https://cdn.shopify.com/s/files/1/0790/2586/4977/files/';
                      var indexRange = [2, 15];
                      var maxLength = indexRange[1] - indexRange[0] + 1;
                      var eleContainer = document.getElementById('lightContainer');

                      var windowWidth = $(window).width();

                      var imgBg = new Image();
                      if (windowWidth > 999) {
                        // 当窗口宽度大于999px时加载的图片系列
                        imgBg.src = imgPath + 'p-1.jpg';
                      } else {
                        // 当窗口宽度小于等于999px时加载的图片系列
                        imgBg.src = imgPath + 'm-1.jpg';
                      }

                      // 存储预加载的DOM对象和长度信息
                      var store = {
                        length: 0,
                      };

                      // 图片序列预加载
                      for (var start = indexRange[0]; start <= indexRange[1]; start++) {
                        (function (index) {
                          var img = new Image();
                          img.classList.add('screen');
                          img.onload = function () {
                            store.length++;
                            // 存储预加载的图片对象
                            store[index] = this;
                            play();
                          };
                          img.onerror = function () {
                            store.length++;
                            play();
                          };
                          if (windowWidth > 999) {
                            // 当窗口宽度大于999px时加载的图片系列
                            img.src = imgPath + 'p-' + index + '.jpg';
                          } else {
                            // 当窗口宽度小于等于999px时加载的图片系列
                            img.src = imgPath + 'm-' + index + '.jpg';
                          }
                        })(start);
                      }

                      var play = function () {
                        // loading进度
                        var percent = Math.round((100 * store.length) / maxLength);
                        // 全部加载完毕，无论成功还是失败
                        if (imgBg.complete && percent == 100) {
                          var index = indexRange[0];
                          eleContainer.innerHTML = '';
                          // 依次append图片对象
                          var step = function () {
                            if (store[index - 1]) {
                              eleContainer.removeChild(store[index - 1]);
                            }
                            eleContainer.appendChild(store[index]);
                            // 序列增加
                            index++;
                            // 如果超过最大限制
                            if (index <= indexRange[1]) {
                              setTimeout(step, 60);
                            }
                          };
                          // 等100%动画结束后执行播放
                          setTimeout(step, 60);
                        }
                      };
                    }

                    // 播放组件
                    (function () {
                      const $section = $('.p-mod-bv'),
                        $mkVideoPlay = $section.find('.mk-video-play'),
                        $popPlayer = $section.find('.popup-player'),
                        $btnVideoWrapper = $section.find('.btn-video-wrapper'),
                        $smallVideo = $section.find('.btn-video-bg'),
