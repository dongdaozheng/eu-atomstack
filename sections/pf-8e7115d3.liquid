                    @media screen and (max-width: 600px) {
                      :root {
                        /* px换算vw */
                        --px2vw: calc(100 / 390);
                        /* 浏览器左右空白间距 */
                        --wrapper-gutter: calc(var(--px2vw) * 15vw);
                        /* 15px; */
                        /* 模块上下间距 */
                        --wrapper-section-padding-top: calc(var(--px2vw) * 65vw);
                        /* 65px; */
                        --wrapper-section-padding-bottom: calc(var(--px2vw) * 65vw);
                        /* 65px; */
                        /* 字号 */
                        --warpper-font-xxx-small: calc(var(--px2vw) * 12vw);
                        /* 12px; */
                        --warpper-font-xx-small: calc(var(--px2vw) * 14vw);
                        /* 14px; */
                        --warpper-font-x-small: calc(var(--px2vw) * 16vw);
                        /* 16px; */
                        --warpper-font-small: calc(var(--px2vw) * 18vw);
                        /* 18px; */
                        --warpper-font-medium: calc(var(--px2vw) * 24vw);
                        /* 24px; */
                        --warpper-font-large: calc(var(--px2vw) * 32vw);
                        /* 32px; */
                        --warpper-font-x-large: calc(var(--px2vw) * 36vw);
                        /* 36px; */
                        --warpper-font-xx-large: calc(var(--px2vw) * 42vw);
                        /* 42px; */
                        --warpper-font-xxx-large: calc(var(--px2vw) * 45vw);
                        /* 45px; */
                      }

