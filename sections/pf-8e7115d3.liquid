                      setVideoUrl();
                      bindEvent();

                      function setVideoUrl() {
                        const smallVideoUrl = $mkVideoPlay.attr('data-small-video-url'),
                          bigVideoUrl = $mkVideoPlay.attr('data-big-video-url');
                        $smallVideo.attr('src', smallVideoUrl);
                        $bigVideo.attr('src', bigVideoUrl);
