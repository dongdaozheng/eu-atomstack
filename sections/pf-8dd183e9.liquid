document.documentElement.style.scrollPaddingTop = `${header.offsetHeight +  announcementBar.offsetHeight}px`

function throttle(fn, delay) {
  let timer = null
  return function () {
    if (timer) {
      return
    }
    timer = setTimeout(() => {
      fn.apply(this, arguments)
      timer = null
    }, delay)
  }
}

