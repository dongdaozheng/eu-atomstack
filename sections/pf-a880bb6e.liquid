var prodSelectDivs = document.querySelectorAll('select.prodSelect');
//设置选中选项集合，并设置默认值
var selectedOpts = new Array('a20Pro', 'a30Pro', 'a40Pro');
//读取comparison_data.json文件
var xhr = new XMLHttpRequest();
xhr.open("GET", "https://cdn.shopify.com/s/files/1/0790/2586/4977/files/comparison_data.json?v=1713438486", true);
xhr.send();
xhr.onload = function () {
  if (xhr.status === 200) {
    //读取成功
    var data = JSON.parse(xhr.responseText);//JSON数据
    var productOpt = data.productOpt; //下拉框数据
    var productInfo = data.data; //产品数据
    // console.log(productInfo);
    //遍历下拉框外部容器集合
    prodSelectDivs.forEach(function (prodSelectDiv, index) {
      var prodSelect = prodSelectDiv
      //初始化清空下拉框内容
      // console.log("prodSelect: " + prodSelect);
      prodSelect.innerHTML = '';
      //遍历数据，动态设置下拉框选项
      productOpt.forEach(function (data) {
        //console.log(data.category);
        //console.log(data.options);
        // console.log("selectedOpts:" + selectedOpts);
        prodSelect.appendChild(createOptgroup(data.category, data.options, index, selectedOpts));
      });
      //渲染产品参数数据
      renderData(prodSelect.value, productInfo, index + 1);

      // 添加选择事件监听器
      prodSelect.addEventListener("change", function () {
        // 获取选中的值
        var selectedValue = prodSelect.value;
        selectedOpts.some((product, idx) => {
          selectedOpts[index] = selectedValue;
          renderData(selectedValue, productInfo, index + 1);
        });
      });

    });
  } else {
    //读取文件失败弹窗
    alert("Data loading failed, please refresh the page or contact us.");
  }
};

// 复用的函数来生成 <optgroup> 元素和选项
function createOptgroup(category, options, index, selectedOpts) {
  var optgroup = document.createElement("optgroup");
  optgroup.label = category;
  options.forEach(function (option) {
    var newOption = new Option(option[1], option[0]);
    optgroup.appendChild(newOption);
  });

  // if (category === "Open Frame" && index === 0) {
  //   optgroup.querySelector(`option[value="a20Pro"]`).selected = true;
  // } else if (category === "Open Frame" && index === 1) {
  //   optgroup.querySelector(`option[value="a30Pro"]`).selected = true;
  // } else if (category === "Open Frame" && index === 2) {
  //   optgroup.querySelector(`option[value="a40Pro"]`).selected = true;
  // }
  // console.log("selectedOpts[" + index + "]: " + selectedOpts[index]);
  if (category === "Open Frame") {
    optgroup.querySelector(`option[value="${selectedOpts[index]}"]`).selected = true;
  }
  return optgroup;
}

//数据渲染函数
function renderData(optionValue, productInfo, index) {
  // console.log("optionValue: " + optionValue);
  // console.log("productInfo: " + JSON.stringify(productInfo));
  //console.log("index: " + index);
  var prodParent = document.getElementById("product" + index); //产品的父标签
  var prodChilds = prodParent.children; //所有产品标签
  //循环先将所有产品隐藏
  for (var i = 0; i < prodChilds.length; i++) {
    var prodChild = prodChilds[i];
    prodChild.style.display = "none";
  }

  //选中的产品容器
  var selProduct = prodParent.querySelector("#" + optionValue);

  productInfo.forEach(function (data) {
    if (optionValue === data.id) {
      // 显示选中产品内容
      selProduct.style.display = 'block';
      var productParams = data.data;
      let keys = Object.keys(productParams);
      for (let i in keys) {
        //console.log(productParams[keys[i]]);
        var objKeys = Object.keys(productParams[keys[i]]);
        for (let j in objKeys) {
          if (keys[i] === "accessories") {
            setParam(productParams[keys[i]], objKeys[j], index, true);
          } else {
            setParam(productParams[keys[i]], objKeys[j], index, false);
          }
        }
      }
    }
  });
}

function setParam(data, key, index, isAccessory) {
  var param = document.getElementById(key + index);
  var accTitle = document.getElementById(key + "_title" + index);
  var accDescription = document.getElementById(key + "_description" + index);
  if (isAccessory) {
    var accessoryInfo = data[key];
    if (accessoryInfo.title === "") {
      accTitle.style.display = "none";
      //accTitle.style.setProperty("display", "none", "important");
      accTitle.innerHTML = "——";
      accDescription.innerHTML = "——";
    } else {
      accTitle.style.display = "block";
      // 创建一个 <a> 标签元素
      var linkElement = document.createElement("a");

      // 设置 <a> 标签的 href 属性和文本内容
      linkElement.href = accessoryInfo.link;
      linkElement.textContent = accessoryInfo.title;

      // 将 <a> 标签添加到元素的 innerHTML 中
      accTitle.innerHTML = ""; // 清空元素的内容
      accTitle.appendChild(linkElement);
      accDescription.innerHTML = accessoryInfo.description;
    }
  } else {
    if (key === "highlights") {
      param.innerHTML = "";
      data[key].forEach(function (item) {
        var listItem = document.createElement("li");
        listItem.textContent = item;
        param.appendChild(listItem);
      });
    } else if (key === "software") {
      param.innerHTML = "";
      // 创建一个 <a> 标签元素
      for (i in data[key]) {
        var aElement = document.createElement("a");
        // 设置 <a> 标签的 href 属性和文本内容
        aElement.href = data[key][i].link;
        aElement.textContent = data[key][i].title;
        param.appendChild(aElement);
        //console.log("iiiiiiiiii: " + i);
        if (i < data[key].length - 1) {
          var brElement = document.createElement("br");
          //spanElement.textContent = ", ";
          param.appendChild(brElement);
        }
      }
      //console.log("param-after: " + param);
    } else {
      if (data[key] === "") {
        param.innerHTML = "——";
      } else {
        param.innerHTML = data[key];
      }
    }
  }
}

