//通过判断屏幕位置动态选中对应的侧边按钮
document.addEventListener("DOMContentLoaded", function () {
  const sidebar = document.getElementById("sidebar");

  const overview = document.getElementById("overview");
  const features = document.getElementById("features");
  const software = document.getElementById("software");
  const appilication = document.getElementById("appilication");
  const accessories = document.getElementById("accessories");

  const btnOverview = document.getElementById("btn-overview");
  const btnFeatures = document.getElementById("btn-features");
  const btnSoftware = document.getElementById("btn-software");
  const btnAppilication = document.getElementById("btn-appilication");
  const btnAccessories = document.getElementById("btn-accessories");

  const btnOverviewh = document.getElementById("btn-overview-h");
  const btnFeaturesh = document.getElementById("btn-features-h");
  const btnSoftwareh = document.getElementById("btn-software-h");
  const btnAppilicationh = document.getElementById("btn-appilication-h");
  const btnAccessoriesh = document.getElementById("btn-accessories-h");

  var w = window.innerHeight;
  var screenHeightThreshold = 160;
  var currentSlide = 0; // 记录当前轮播索引

  window.addEventListener("scroll", function () {
    // 当滚动位置超过Overview元素时显示侧边栏
    if (getRect(overview).bottom <= w && getRect(accessories).bottom >= w - 300) {
      sidebar.style.display = "block";
      removeAct();
      addAct(btnOverview, btnOverviewh);
      if (getRect(overview).top <= screenHeightThreshold && getRect(overview).bottom >= screenHeightThreshold) {
        //激活Overview按钮样式
        removeAct();
        addAct(btnOverview, btnOverviewh);
      } else if (getRect(features).top <= screenHeightThreshold && getRect(features).bottom >= screenHeightThreshold) {
        //激活Features按钮样式
        removeAct();
        addAct(btnFeatures, btnFeaturesh);
      } else if (getRect(software).top <= screenHeightThreshold && getRect(software).bottom >= screenHeightThreshold) {
        //激活Software按钮样式
        removeAct();
        addAct(btnSoftware, btnSoftwareh);
      } else if (getRect(appilication).top <= screenHeightThreshold && getRect(appilication).bottom > screenHeightThreshold) {
        //激活Appilication按钮样式
        removeAct();
        addAct(btnAppilication, btnAppilicationh);
      } else if (getRect(accessories).top <= screenHeightThreshold && getRect(accessories).bottom > screenHeightThreshold) {
        //激活Accessories按钮样式
        removeAct();
        addAct(btnAccessories, btnAccessoriesh);
      }
    } else {
      sidebar.style.display = "none";
    }
  });
  // 获取元素的位置
  function getRect(element) {
    return element.getBoundingClientRect();
  }

  function removeAct() {
    btnOverview.classList.remove("btn-active");
    btnFeatures.classList.remove("btn-active");
    btnSoftware.classList.remove("btn-active");
    btnAppilication.classList.remove("btn-active");
    btnAccessories.classList.remove("btn-active");

    btnOverviewh.classList.remove("btn-active-h");
    btnFeaturesh.classList.remove("btn-active-h");
    btnSoftwareh.classList.remove("btn-active-h");
    btnAppilicationh.classList.remove("btn-active-h");
    btnAccessoriesh.classList.remove("btn-active-h");
  }


  //添加选中样式
  function addAct(element1, element2) {
    element1.classList.add("btn-active");
    element2.classList.add("btn-active-h");
  }

  function autoplaySlide(element, thisSlide) {
    var sliderContainer = document.getElementById("slide-container").children[0];
    var parentElement = element.parentNode;
    var slideWidth = parentElement.offsetWidth;
    var moveSlide = currentSlide - thisSlide;
    if ((currentSlide === 2 && moveSlide <= 0) || (currentSlide === 3 && moveSlide <= 1) || (currentSlide === 4 && moveSlide <= 2)) {
      currentSlide = thisSlide;
      console.log("不变不变不变：" + currentSlide);
    } else {
      var translateValue = moveSlide * slideWidth + 'px';
      sliderContainer.style.transform = 'translateX(' + translateValue + ')';
      console.log("左移左移左移：" + translateValue);
      currentSlide = thisSlide;
    }
  }
});</script>
